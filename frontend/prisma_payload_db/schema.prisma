generator client {
  provider = "prisma-client"
  output   = "./payload_db"
}

datasource db {
  provider = "sqlite"
  url      = env("PAYLOAD_DB_URL")
}

model raw {
  id               Int    @id @default(autoincrement())
  flow_id          Int
  count            Int?
  server_to_client Int?
  blob             Bytes?

  @@unique([flow_id, count], map: "sqlite_autoindex_raw_1")
  @@index([flow_id])
}
